{
  "name": "wedevelop-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cloudinary/url-gen": "^1.0.0",
    "@loadable/component": "^5.15.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.3",
    "bootstrap": "^5.1.0",
    "classnames": "^2.2.6",
    "firebase": "^9.0.1",
    "formik": "^2.2.6",
    "lodash": "^4.17.21",
    "next": "^12.0.8",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.6.1",
    "react-calendly": "^1.3.1",
    "react-dom": "^17.0.1",
    "react-flags-select": "^2.1.2",
    "react-loader-spinner": "^4.0.0",
    "react-modal": "^3.13.1",
    "react-prerendered-component": "^1.2.4",
    "react-router-hash-link": "^2.4.0",
    "react-select": "^4.3.1",
    "react-slideshow-image": "^3.5.0",
    "react-step-wizard": "^5.3.9",
    "react-text-loop": "^2.3.0",
    "rectangles": "^0.0.4",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.0",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "next start",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "prebuild": "rimraf build",
    "build": "NODE_ENV=production next build && react-snap && rimraf ./build/storage",
    "postbuild": "npm run add-http2-push-headers && dotenv -e .env.production npm run build-sitemap",
    "add-http2-push-headers": "ncp firebase.json firebase.json.backup && node ./add_http2_push_headers.js",
    "build-sitemap": "react-snap-sitemap --base-url=$REACT_APP_BASE_URL/",
    "build-testing": "ncp .testing_env .env.production && npm run build",
    "build-preview": "REACT_APP_PREVIEW_BUILD=preview npm run build-testing",
    "build-production": "ncp .production_env .env.production && npm run build",
    "test": "next test",
    "eject": "next eject",
    "deploy-functions-production": "firebase deploy --only functions --project wedevelop-site",
    "deploy-functions-testing": "firebase deploy --only functions --project wedevelop-site-testing",
    "deploy-hosting-production": "firebase deploy --project wedevelop-site --only hosting:production",
    "deploy-hosting-testing": "firebase deploy --project wedevelop-site-testing --only hosting:testing",
    "cleanup-deploy-build": "rimraf -rf firebase.json.backup .env.production build node_modules/.cache",
    "deploy-hosting-preview": "firebase hosting:channel:deploy --project wedevelop-site-testing --only testing"
  },
  "reactSnap": {
    "skipThirdPartyRequests": true,
    "http2PushManifest": true,
    "asyncScriptTags": true,
    "inlineCss": true,
    "include": [
      "/privacy-policy",
      "/staff-augmentation",
      "/web-developers",
      "/hire-developers",
      "/web-coders",
      "/hire-javascript-developers",
      "/hire-net-developers",
      "/website-builders",
      "/web-programmers",
      "/about-us",
      "/career",
      "/follow-up",
      "/offshore-web-development",
      "/nearshore-net-developers",
      "/web-development-outsourcing",
      "/nearshore-web-development",
      "/success/confirm",
      "/nearshore-software-outsourcing",
      "/devops",
      "/it-outsourcing",
      "/it-staffing",
      "/mobile",
      "/software-development"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fudohq/react-snap-sitemap": "^0.1.5",
    "dotenv-cli": "^4.0.0",
    "firebase-tools": "^9.5.0",
    "import-glob-loader": "^1.1.0",
    "ncp": "^2.0.0",
    "react-snap": "^1.23.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "standard": "^16.0.3"
  }
}
