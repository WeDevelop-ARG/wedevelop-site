{
  "name": "wedevelop-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.3",
    "bootstrap": "^5.1.0",
    "classnames": "^2.2.6",
    "firebase": "^9.0.1",
    "formik": "^2.2.6",
    "lodash": "^4.17.21",
    "next": "^12.0.8",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.6.1",
    "react-calendly": "^1.3.1",
    "react-dom": "^17.0.1",
    "react-flags-select": "^2.1.2",
    "react-loader-spinner": "^4.0.0",
    "react-modal": "^3.13.1",
    "react-prerendered-component": "^1.2.4",
    "react-router-hash-link": "^2.4.0",
    "react-select": "^4.3.1",
    "react-slideshow-image": "^3.5.0",
    "react-step-wizard": "^5.3.9",
    "react-text-loop": "^2.3.0",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.0",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "next start",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "prebuild": "rimraf build",
    "build": "NODE_ENV=production next build",
    "build-testing": "ncp .testing_env .env.production && npm run build",
    "build-production": "ncp .production_env .env.production && npm run build",
    "test": "next test",
    "eject": "next eject",
    "deploy-functions-production": "firebase deploy --only functions --project wedevelop-site",
    "deploy-functions-testing": "firebase deploy --only functions --project wedevelop-site-testing",
    "deploy-hosting-production": "firebase deploy --project wedevelop-site --only hosting:production",
    "deploy-hosting-testing": "firebase deploy --project wedevelop-site-testing --only hosting:testing",
    "cleanup-deploy-build": "rimraf -rf firebase.json.backup .env.production build node_modules/.cache",
    "deploy-hosting-preview": "firebase hosting:channel:deploy --project wedevelop-site-testing --only testing"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "dotenv-cli": "^4.0.0",
    "firebase-tools": "^9.5.0",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "standard": "^16.0.3"
  }
}
